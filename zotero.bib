@article{low299,
  title = {Guidelines for {{Perioperative Care}} in {{Esophagectomy}}: {{Enhanced Recovery After Surgery}} ({{ERAS}}\textregistered ) {{Society Recommendations}}},
  shorttitle = {Guidelines for {{Perioperative Care}} in {{Esophagectomy}}},
  author = {Low, Donald E. and Allum, William and De Manzoni, Giovanni and Ferri, Lorenzo and Immanuel, Arul and Kuppusamy, MadhanKumar and Law, Simon and Lindblad, Mats and Maynard, Nick and Neal, Joseph and Pramesh, C. S. and Scott, Mike and Mark Smithers, B. and Addor, Val{\'e}rie and Ljungqvist, Olle},
  year = {2019},
