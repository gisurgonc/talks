  keywords = {Abdominal Fat,Adipose Tissue,Adiposity,Automation,Body Composition,Female,Humans,Image Processing; Computer-Assisted,Liver,Male,Middle Aged,ML,Muscles,Reproducibility of Results,Tomography; X-Ray Computed},
