@article{nakashima492,
  title = {Skeletal {{Muscle Loss After Esophagectomy Is}} an {{Independent Risk Factor}} for {{Patients}} with {{Esophageal Cancer}}},
  author = {Nakashima, Yuichiro and Saeki, Hiroshi and Hu, Qingjiang and Tsuda, Yasuo and Zaitsu, Yoko and Hisamatsu, Yuichi and Ando, Koji and Kimura, Yasue and Oki, Eiji and Mori, Masaki},
  year = {2020},
