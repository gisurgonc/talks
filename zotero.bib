  author = {Zhang, S. S. and Yang, H. and Luo, K. J. and Huang, Q. Y. and Chen, J. Y. and Yang, F. and Cai, X. L. and Xie, X. and Liu, Q. W. and Bella, A. E. and Hu, R. G. and Wen, J. and Hu, Y. and Fu, J. H.},
  year = {2013},
