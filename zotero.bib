@article{blauwhoff-buskermolen1339,
  title = {Loss of {{Muscle Mass During Chemotherapy Is Predictive}} for {{Poor Survival}} of {{Patients With Metastatic Colorectal Cancer}}},
  author = {{Blauwhoff-Buskermolen}, Susanne and Versteeg, Kathelijn S. and {de van der Schueren}, Marian A. E. and {den Braver}, Nicole R. and Berkhof, Johannes and Langius, Jacqueline A. E. and Verheul, Henk M. W.},
  year = {2016},
